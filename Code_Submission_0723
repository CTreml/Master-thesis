weighted.mean(ASFL6lfinbuff$fin_beh,ASFL6lfinbuff$wght)

weighted.mean(ASFL6hfinbuff$fin_att,ASFL6hfinbuff$wght)
weighted.mean(ASFL6lfinbuff$fin_att,ASFL6lfinbuff$wght)
