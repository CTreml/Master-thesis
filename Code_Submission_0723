                            selfassessment=rep(c('Ref/DonÂ´t know','Low', 'Average', 'High'), times=3),
                            share=c(0.04768587,0.04254538,0.34443552,0.5653332,0.02188622,0.1390004,0.4149923,0.4241211,0.02553137,0.2407209,0.3976323,0.3361154))
