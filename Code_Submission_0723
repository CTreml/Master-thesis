table(ASFL$QF12_7_1o)
QF12_7_1_1<- ifelse(ASFL$QF12_7_1o == "Geld von meiner Mutter", 1, 0)
QF12_7_1_2<- ifelse(ASFL$QF12_7_1o == "Momentan helfen mir meine Eltern und Geschwister, da ich gerade arbeitslos bin. Bin auch sehr froh darÃÂ¼ber, so kann ich auch ein bisschen sparen", 1, 0)
QF12_7_1_3<- ifelse(ASFL$QF12_7_1o == "Verwandte um UnterstÃÂ¼tzung bitten", 1, 0)
QF12_7 <- QF12_7_1_1+QF12_7_1_2+QF12_7_1_3
QF12_7fin<- ifelse(QF12_7 == "0", 1, 0)
